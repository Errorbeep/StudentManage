<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="672.0" styleClass="box" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="priv.matrix.student.controller.PersonalController">
    <VBox layoutX="70.0" layoutY="40.0" prefHeight="321.0" prefWidth="532.0" spacing="20" AnchorPane.leftAnchor="70.0" AnchorPane.topAnchor="40.0">
      <HBox prefHeight="28.0" prefWidth="532.0" spacing="50.0">
         <children>
              <Label fx:id="adminName" styleClass="font-title" text="姓名" />
         </children>
      </HBox>
      <HBox prefHeight="37.0" prefWidth="532.0" spacing="40.0">
         <children>
            <Label prefHeight="33.0" prefWidth="39.0" text="账号" />
            <TextField fx:id="adminAccount" editable="false" prefHeight="30.0" prefWidth="319.0" styleClass="gray-theme" text="账号" />
         </children>
      </HBox>
      <HBox prefHeight="41.0" prefWidth="532.0" spacing="40.0">
         <children>
            <Label prefHeight="34.0" prefWidth="40.0" text="密码" />
              <TextField fx:id="adminPassword" editable="false" prefHeight="30.0" prefWidth="309.0" styleClass="gray-theme" text="密码" />
         </children>
      </HBox>
        <HBox prefHeight="40.0" prefWidth="532.0" spacing="30">
            <Button onAction="#edit" text="修改">
            <styleClass>
               <String fx:value="green-theme" />
               <String fx:value="btn-radius" />
            </styleClass></Button>
            <Button onAction="#save" text="保存">
            <styleClass>
               <String fx:value="blue-theme" />
               <String fx:value="btn-radius" />
            </styleClass></Button>
        </HBox>
    </VBox>
</AnchorPane>
